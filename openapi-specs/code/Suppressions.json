                  "value": {
                    "comment": "Policy is irrelevant.",
                    "expirationTime": 1834303033687,
                    "origin": "Platform",
                    "suppressionType": "Policy"
                  }
                },
                "Example 2": {
                  "value": {
                    "comment": "Ignored.",
                    "expirationTime": 1834303033687,
                    "origin": "Platform",
                    "suppressionType": "Tags",
                    "tags": [{ "key": "team", "value": "knights" }]
