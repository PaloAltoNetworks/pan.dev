        "type": 301
      },
      {
        "source": "/cdl/logforwarding/docs",
        "destination": "/cdl/docs/log-forwarding",
        "type": 301
      },
      {
        "source": "/cdl/logforwarding/docs/:path",
        "destination": "/cdl/docs/log-forwarding/:path",
        "type": 301
      },
      {
        "source": "/cdl/docs/getstarted_lf",
