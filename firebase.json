        "destination": "/sase/docs",
        "type": 301
      },
      {
        "source": "/sase/api/auth/authservice",
        "destination": "/sase/api/auth",
        "type": 301
      },
      {
        "source": "/sase/docs/sdwan/sdwan",
        "destination": "/sdwan/docs",
        "type": 301
      },
      {
        "source": "/sase/docs/sdwan/:path",
