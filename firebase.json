{
  "hosting": {
    "headers": [
      {
        "source": "/service-worker.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ],
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/panos",
        "destination": "https:/panos.pan.dev",
        "type": 301
      },
      {
        "source": "/strata",
        "destination": "https:/panos.pan.dev",
        "type": 301
      },
      {
        "source": "/xsoar",
        "destination": "https://xsoar.pan.dev",
        "type": 301
      },
      {
        "source": "/demisto",
        "destination": "https://xsoar.pan.dev",
        "type": 301
      },
      {
        "source": "/prisma",
        "destination": "https://prisma.pan.dev",
        "type": 301
      },
      {
        "source": "/cortex",
        "destination": "https://cortex.pan.dev",
        "type": 301
      },
      {
        "source": "/xdr",
        "destination": "https://cortex.pan.dev",
        "type": 301
      },
      {
        "source": "/xpanse",
        "destination": "https://cortex.pan.dev",
        "type": 301
      },
      {
        "source": "/github",
        "destination": "https://gallery.pan.dev",
        "type": 301
      },
      {
        "source": "/blog",
        "destination": "https://medium.com/palo-alto-networks-developer-blog",
        "type": 301
      }
    ]
  }
}
